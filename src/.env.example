# ==========================================
# KONFIGURASI BINANCE
# ==========================================
# API Key Utama (Diperlukan untuk trading akun riil)
# Dapatkan di: https://www.binance.com/en/my/settings/api-management
BINANCE_API_KEY=

# Secret Key Utama
BINANCE_SECRET_KEY=

# API Key Testnet (Opsional: hanya jika menggunakan mode testnet)
# Dapatkan di: https://testnet.binancefuture.com/
BINANCE_TESTNET_KEY=

# Secret Key Testnet
BINANCE_TESTNET_SECRET=

# ==========================================
# KONFIGURASI TELEGRAM (NOTIFIKASI)
# ==========================================
# Token Bot Telegram (Dapat dari @BotFather)
TELEGRAM_TOKEN=

# Chat ID tujuan notifikasi (Gunakan @userinfobot untuk cek ID)
TELEGRAM_CHAT_ID=

# (Opsional) Thread ID jika menggunakan Group dengan Topics
# Kosongkan jika tidak menggunakan fitur topic
TELEGRAM_MESSAGE_THREAD_ID=

# ==========================================
# KONFIGURASI AI & DATA EKSTERNAL
# ==========================================
# API Key untuk Provider AI (misal: OpenRouter, OpenAI)
AI_API_KEY=

# API Key CoinMarketCap (Untuk analisis fundamental/berita)
CMC_API_KEY=

# ==========================================
# KONFIGURASI KHUSUS SENTIMEN (OPSIONAL)
# ==========================================
# Token Bot untuk notifikasi sentimen (Bisa sama dengan token utama atau bot berbeda)
TELEGRAM_TOKEN_SENTIMENT=

# Chat ID untuk notifikasi sentimen
TELEGRAM_CHAT_ID_SENTIMENT=

# (Opsional) Thread ID untuk sentimen (Jika menggunakan Topics)
TELEGRAM_MESSAGE_THREAD_ID_SENTIMENT=